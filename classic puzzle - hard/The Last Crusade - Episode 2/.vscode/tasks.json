{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "haxe",
			"args": "active configuration",
			"problemMatcher": [
				"$haxe-absolute",
				"$haxe",
				"$haxe-error",
				"$haxe-trace"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"label": "haxe: active configuration"

		},
		{
			"type": "hxml",
			"label": "Build",
			"file": "simulator.hxml",
			"group": {
				"kind": "build",
				"isDefault": false
			}
		}
	]
}
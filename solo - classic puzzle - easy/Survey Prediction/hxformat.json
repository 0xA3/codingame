{
	"emptyLines": {
		"afterLeftCurly": "keep",
		"classEmptyLines": {
			"beginType": 1
		}
	},
	"sameLine": {
		"caseBody": "same",
		"catchBody": "keep",
		"doWhileBody": "keep",
		"elseBody": "keep",
		"elseIf": "keep",
		"forBody": "keep",
		"functionBody": "keep",
		"ifBody": "keep",
		"whileBody": "keep"
	},
	"whitespace": {
		"parenConfig": {
			"anonFuncParamParens": {
				"openingPolicy": "after",
				"closingPolicy": "around",
				"removeInnerWhenEmpty": true
			},
			"callParens": {
				"openingPolicy": "after",
				"closingPolicy": "around",
				"removeInnerWhenEmpty": true
			},
			"funcParamParens": {
				"openingPolicy": "after",
				"closingPolicy": "around",
				"removeInnerWhenEmpty": true
			},
			"forLoopParens": {
				"openingPolicy": "after",
				"closingPolicy": "around",
				"removeInnerWhenEmpty": true
			},
			"ifConditionParens": {
				"openingPolicy": "after",
				"closingPolicy": "around",
				"removeInnerWhenEmpty": true
			},
			"whileConditionParens": {
				"openingPolicy": "after",
				"closingPolicy": "around",
				"removeInnerWhenEmpty": true
			},
			"expressionParens": {
				"openingPolicy": "after",
				"closingPolicy": "around",
				"removeInnerWhenEmpty": true
			}
		},
		"bracesConfig": {
			"anonTypeBraces": {
				"openingPolicy": "after",
				"closingPolicy": "before",
				"removeInnerWhenEmpty": true
			},
			"blockBraces": {
				"openingPolicy": "after",
				"closingPolicy": "before",
				"removeInnerWhenEmpty": true
			},
			"objectLiteralBraces": {
				"openingPolicy": "after",
				"closingPolicy": "before",
				"removeInnerWhenEmpty": true
			},
			"unknownBraces": {
				"openingPolicy": "after",
				"closingPolicy": "before",
				"removeInnerWhenEmpty": true
			}
		},
		"ifPolicy": "before",
		"whilePolicy": "before",
		"forPolicy": "before",
		"compressSuccessiveParenthesis": true
	},
	"wrapping": {
		"maxLineLength": 999,
		"functionSignature": {
			"defaultWrap": "noWrap",
			"rules": [
				{
					"conditions": [
						{
							"cond": "totalItemLength >= n",
							"value": 130
						}
					],
					"type": "onePerLine",
					"additionalIndent": 1
				}
			]
		}
	}
}